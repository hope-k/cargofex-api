
{
    "version": 2,
    "builds": [
      {
        "src": "courier/wsgi.py",
        "use": "@vercel/python",
        "config": {"maxLambdaSize": "15mb"}
      }
     
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "courier/wsgi.py"
      },
      {
        "src": "/static/(.*)",
        "dest": "/static/$1"
      }
    ]
  }